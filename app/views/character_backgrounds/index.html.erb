<h1>Listing character_backgrounds</h1>

<table>
  <thead>
    <tr>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @character_backgrounds.each do |character_background| %>
      <tr>
        <td><%= link_to 'Show', character_background %></td>
        <td><%= link_to 'Edit', edit_character_background_path(character_background) %></td>
        <td><%= link_to 'Destroy', character_background, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

