<h1>Listing character_class_types</h1>

<table>
  <thead>
    <tr>
      <th>Class Name</th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @character_class_types.each do |cat| %>
      <tr>
        <td><%= cat.character_class_name %></td>
        <td><%= link_to 'Show', cat %></td>
        <td><%= link_to 'Edit', edit_character_class_type_path(cat) %></td>
        <td><%= link_to 'Destroy', cat, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Character class type', new_character_class_type_path %>
